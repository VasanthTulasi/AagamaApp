<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.vasanth.hambutton">

    <application
        tools:replace="android:allowBackup"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:logo="@drawable/launch1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        >

        <activity android:name=".SplashScreen"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity android:name=".Walkthrough" android:screenOrientation="portrait"/>
        <activity android:name=".HamButtonActivity" android:screenOrientation="portrait"/>
        <activity android:name=".MainActivityDept" android:screenOrientation="portrait"/>
        <activity android:name=".branch.CseActivity" android:screenOrientation="portrait"/>
        <activity android:name=".branch.EceActivity" android:screenOrientation="portrait"/>
        <activity android:name=".MainActivityForZones" android:screenOrientation="portrait"/>


    </application>

</manifest>